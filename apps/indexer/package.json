{
  "name": "@tickasting/indexer",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "db:generate": "prisma generate --schema=../api/prisma/schema.prisma"
  },
  "dependencies": {
    "fastify": "^5.2.0",
    "@tickasting/shared": "workspace:*",
    "@prisma/client": "^5.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.0",
    "eslint": "^9.17.0",
    "pino-pretty": "^13.0.0",
    "prisma": "5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0",
    "vitest": "^2.1.8"
  }
}
